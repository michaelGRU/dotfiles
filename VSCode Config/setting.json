{
    "window.zoomLevel": 1,
    "files.autoSave": "afterDelay",
    "workbench.colorTheme": "Gruvbox Dark Hard-flymin",
    "workbench.startupEditor": "none",
    "editor.lineHeight": 0,
    "editor.fontSize": 15,
    "r.bracketedPaste": true,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "notebook.cellToolbarLocation": {
        "jupyter-notebook": "right"
    },
    "window.title": "${dirty}${activeEditorShort}${separator}${rootName}${separator}$",
    "editor.cursorBlinking": "solid",
    "multiCommand.commands": [
        {
            "command": "clear_vsc",
            "sequence": [
                "workbench.action.toggleActivityBarVisibility",
                "workbench.action.toggleSidebarVisibility"
            ]
        }
    ],
    "editor.glyphMargin": true,
    "window.customMenuBarAltFocus": false,
    "window.enableMenuBarMnemonics": false,
    "editor.scrollbar.verticalScrollbarSize": 0,
    "terminal.integrated.inheritEnv": false,
    "editor.parameterHints.enabled": false,
    "editor.fontFamily": "'monospace', chiller",
    "editor.wordWrap": "on",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.fontSize": 14,
    "workbench.colorCustomizations": {
        "terminal.foreground": "#66FF66",
        "terminalCursor.background": "#D5C4A1",
        "terminalCursor.foreground": "#D5C4A1",
        "terminal.ansiBlack": "#32302F",
        "terminal.ansiBlue": "#83A598",
        "terminal.ansiBrightBlack": "#665C54",
        "terminal.ansiBrightBlue": "#83A598",
        "terminal.ansiBrightCyan": "#8EC07C",
        "terminal.ansiBrightGreen": "#B8BB26",
        "terminal.ansiBrightMagenta": "#D3869B",
        "terminal.ansiBrightRed": "#FB4934",
        "terminal.ansiBrightWhite": "#FBF1C7",
        "terminal.ansiBrightYellow": "#FABD2F",
        "terminal.ansiCyan": "#8EC07C",
        "terminal.ansiGreen": "#B8BB26",
        "terminal.ansiMagenta": "#D3869B",
        "terminal.ansiRed": "#FB4934",
        "terminal.ansiWhite": "#D5C4A1",
        "terminal.ansiYellow": "#FABD2F",
        "statusBar.background": "#1A1A1A",
        "statusBar.noFolderBackground": "#212121",
        "statusBar.debuggingBackground": "#263238",
        "editorLineNumber.activeForeground": "#FFFFFF",
        "notebook.cellBorderColor": "#D5C4A1",
        "notebook.focusedEditorBorder": "#8EC07C",
        "notebook.cellEditorBackground": "#272822",
        "editor.background": "#212121",
        "editorCursor.foreground": "#ff0",
        "editor.selectionBackground": "#118b2a",
        "editor.selectionHighlightBackground": "#ff0000",
        "editor.findMatchBackground": "#00cc44a8",
        "editor.findMatchHighlightBackground": "#ff7b00a1",
    },
    "python.formatting.provider": "black",
    "terminal.integrated.tabs.description": "",
    "terminal.integrated.tabs.title": "${task}${separator}${local}${separator}${cwdFolder}",
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "terminal.external.osxExec": "iTerm.app",
    "liveServer.settings.donotShowInfoMsg": true,
    "git.confirmSync": false,
    "breadcrumbs.enabled": false,
    "python.analysis.completeFunctionParens": true,
    "editor.formatOnSave": true,
    "terminal.integrated.fontFamily": "Inconsolata",
    "editor.acceptSuggestionOnEnter": "smart",
    "files.autoGuessEncoding": true,
    "jupyter.generateSVGPlots": true,
    "notebook.cellToolbarVisibility": "hover",
    "python.defaultInterpreterPath": "/home/michaelgru/anaconda3/envs/coc/bin/python",
    "jupyter.askForKernelRestart": false,
    "editor.cursorWidth": 3,
    "editor.renderLineHighlight": "none",
    "notebook.lineNumbers": "on",
    "editor.inlineSuggest.enabled": true,
    "notebook.undoRedoPerCell": false,
    "notebook.output.textLineLimit": 500,
    "window.menuBarVisibility": "toggle",
    "editor.renderWhitespace": "none",
    "editor.fontWeight": "bold",
    "editor.minimap.enabled": false,
    "explorer.confirmDelete": false,
    "editor.cursorSmoothCaretAnimation": true,
    "editor.tokenColorCustomizations": {
        "[Gruvbox Dark Hard-flymin]": {
            "textMateRules": [
                {
                    "scope": [
                        "comment",
                        "string.quoted.docstring.multi.python"
                    ],
                    "settings": {
                        "fontStyle": "italic",
                        "foreground": "#928374"
                    }
                },
                {
                    "scope": [
                        "keyword.operator.assignment.python"
                    ],
                    "settings": {
                        "foreground": "#fb4934"
                    }
                },
                {
                    "scope": [
                        "string.quoted.single.python"
                    ],
                    "settings": {
                        "foreground": "#66FF66",
            
                    }
                },
                {
                    "scope": ["keyword.control.import.python"],
                    "settings": {
                        "foreground": "#fb4934",
    
                    }
                }, 
                {
                    "scope": ["storage.type.function.python"],
                    "settings": {
                        "foreground": "#fb4934",
                        "fontStyle": "bold"
                    }
                },
                {
                    "scope":["entity.name.function"],
                    "settings": {
                        "foreground": "#fabd2f",
                    }
                },
            ]
        }
    },
    "editor.semanticTokenColorCustomizations": {
        "[Gruvbox Dark Hard-flymin]": {
            "rules": {
                "variable.declaration": "#ebdbb2",
                "variable": "#ebdbb2",
                "parameter.declaration": "#b16286",
                "parameter": "#b16286"
            }
        }
    },
    "workbench.iconTheme": "vscode-icons",
}
